   {
      "body" : "Could you write up a gist that explains what formula for priority you're using?  And maybe talk about how it will handle transactions from old clients (who calculate priority the old way) ?",
      "created_at" : "2012-07-13T00:20:12Z",
      "html_url" : "https://github.com/bitcoin/bitcoin/pull/1240#issuecomment-6951577",
      "id" : 6951577,
      "issue_url" : "https://api.github.com/repos/bitcoin/bitcoin/issues/1240",
      "updated_at" : "2012-07-13T00:20:12Z",
      "url" : "https://api.github.com/repos/bitcoin/bitcoin/issues/comments/6951577",
      "user" : {
         "avatar_url" : "https://avatars.githubusercontent.com/u/331997?v=3",
         "events_url" : "https://api.github.com/users/gavinandresen/events{/privacy}",
         "followers_url" : "https://api.github.com/users/gavinandresen/followers",
         "following_url" : "https://api.github.com/users/gavinandresen/following{/other_user}",
         "gists_url" : "https://api.github.com/users/gavinandresen/gists{/gist_id}",
