      "diff_hunk" : "@@ -57,6 +58,51 @@\n     print('To properly generate man pages, please commit your changes (or discard them), rebuild, then run this script again.')\n     print()\n \n+\n+def parse_and_remove_subsections_from_doc(document: str, subsections_to_remove: list[str]) -> str:\n+    parsed_data = {}\n+    parsed_data['HEADER'], *sections = document.split('.SH')\n+    for section in sections:\n+        section_header, *_ = section.split('\\n')\n+        section_header = section_header.strip()\n+\n+        if section_header == 'OPTIONS':\n+            options_body = section.split('.HP')\n+            for index, option in enumerate(options_body):\n+                for subsection_to_remove in subsections_to_remove:\n+                    if subsection_to_remove in option:\n+                        del options_body[index]\n+            parsed_data[section_header] = '.HP'.join(options_body)\n+        else:\n+            parsed_data[section_header] = section\n+\n+    return '.SH'.join([parsed_data['HEADER'], parsed_data['NAME'], parsed_data['SYNOPSIS'], parsed_data['DESCRIPTION'], parsed_data['OPTIONS'], parsed_data['COPYRIGHT']])\n+\n+\n+def check_build_options(binary: str, document: str) -> str:\n+    config = configparser.ConfigParser()\n+    conffile = os.path.join(builddir, 'test/config.ini')\n+    config.read(conffile)\n+\n+    docs_to_exlude = []\n+    if (binary == 'bitcoind' or binary == 'bitcoin-qt') and not config['components'].getboolean('HAVE_SYSTEM'):\n+        docs_to_exlude += ['shutdownnotify', 'alertnotify', 'startupnotify', 'blocknotify']\n+        return parse_and_remove_subsections_from_doc(document, docs_to_exlude)",
