      "html_url" : "https://github.com/bitcoin/bitcoin/pull/5898#issuecomment-83619186",
      "id" : 83619186,
      "issue_url" : "https://api.github.com/repos/bitcoin/bitcoin/issues/5898",
      "updated_at" : "2015-03-19T15:13:28Z",
      "url" : "https://api.github.com/repos/bitcoin/bitcoin/issues/comments/83619186",
      "user" : {
         "avatar_url" : "https://avatars.githubusercontent.com/u/1095675?v=3",
         "events_url" : "https://api.github.com/users/luke-jr/events{/privacy}",
